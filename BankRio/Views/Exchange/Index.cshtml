@model IEnumerable<Exchange>
@{
    ViewData["Title"] = "Exchange";
}

<br />
<br />
<br />
<h1>@ViewData["Title"]</h1>
<a asp-action="Create" class="btn btn-default">Cadastrar Câmbio</a>


<table class="table table-striped table-hover">
    <thead>
        <tr class="success">
            <td>@Html.DisplayNameFor(model => model.Date)</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(x => item.Date)</td>
            </tr>
        }

    </tbody>
</table>