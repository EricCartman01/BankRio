@model BankRio.Models.ViewModels.ExchangeFormViewModel
@{
    ViewData["Index"] = "Lançar Câmbio";
}
<br />
<br />
<br />
<h4>@ViewData["Index"]</h4>
<div class="row">
    <div class="col-md-4">
        <form asp-action="CREATE">
            <div class="form-group">
                <label asp-for="Exchange.Order" class="control-label"></label>
                <input asp-for="Exchange.Order" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Exchange.Currency" class="control-label"></label>
                <input asp-for="Exchange.Currency" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Exchange.GrossValue" class="control-label"></label>
                <input asp-for="Exchange.GrossValue" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Exchange.Value" class="control-label"></label>
                <input asp-for="Exchange.Value" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Exchange.Cotation" class="control-label"></label>
                <input asp-for="Exchange.Cotation" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Exchange.Comission" class="control-label"></label>
                <input asp-for="Exchange.Comission" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Exchange.NetValue" class="control-label"></label>
                <input asp-for="Exchange.NetValue" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Salvar" class="btn btn-default" />
                <a asp-action="Index" class="btn">Voltar</a>
            </div>
        </form>
    </div>
</div>